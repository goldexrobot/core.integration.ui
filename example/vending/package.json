{
  "name": "ui",
  "version": "1.0.0",
  "description": "Goldex Robot UI",
  "private": true,
  "scripts": {
    "build": "webpack --config webpack.production.js",
    "start": "webpack serve --config webpack.development.js",
    "gen": "openapi --input ../../vending-customer-api.json  --output ./src/service/bot/api --postfix BotService -c fetch --name Client --exportCore false",
    "postgen": "replace-in-file --quiet --configFile=./src/service/bot/string-replace.config.js",
    "mock": "open-api-mocker -s ../../vending-customer-api.json -w"
  },
  "devDependencies": {
    "@types/node": "^17.0.36",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@types/react-input-mask": "^3.0.1",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "dotenv-webpack": "^7.1.0",
    "html-webpack-plugin": "^5.5.0",
    "open-api-mocker": "^1.11.1",
    "openapi-typescript-codegen": "^0.23.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.0",
    "typescript": "^4.7.2",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "isomorphic-fetch": "^3.0.0",
    "line-awesome": "^1.3.0",
    "react": "^18.1.0",
    "react-countdown": "^2.3.2",
    "react-dom": "^18.1.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-spring": "^9.4.5",
    "replace-in-file": "^6.3.5",
    "url": "^0.11.0"
  }
}
